{
  "name": "game-maker",
  "version": "1.0.0",
  "main": "game-maker.js",
  "keywords": [
    "game",
    "LLM",
    "Babylon.js"
  ],
  "author": "uds",
  "license": "MIT",
  "scripts": {
    "watch": "npx shadow-cljs watch app",
    "release": "npx shadow-cljs release app",
    "tailwindcss-watch": "npx tailwindcss -i resources/public/css/styles.css -o resources/public/css/compiled/styles.css --watch",
    "tailwindcss-dev": "npx tailwindcss -i resources/public/css/styles.css -o resources/public/css/compiled/styles.css",
    "tailwindcss-prod": "npx tailwindcss -i resources/public/css/styles.css -o resources/public/css/compiled/styles.css --minify",
    "release-all": "npm run release && npm run tailwindcss-prod",
    "build-report": "npx shadow-cljs run shadow.cljs.build-report app target/build-report.html"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "openai": "4.32.0",
    "babylonjs": "7.0.0",
    "cannon": "0.6.2", 
    "@tailwindcss/aspect-ratio": "0.4.2",
    "@tailwindcss/forms": "0.5.7"
  },
  "devDependencies": {
    "shadow-cljs": "^2.28.3",
    "source-map-support": "0.5.21",
    "tailwindcss": "3.4.3"
  }
}
